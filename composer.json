{
    "name": "bittenbyte/laravel-scout-elasticsearch",
    "description": "ElasticSearch Custom Engine for Laravel Scout",
    "keywords": [
        "laravel",
        "scout",
        "elasticsearch",
        "engine"
    ],
    "homepage": "https://github.com/BittenByte/laravel-scout-elasticsearch",
    "license": "MIT",
    "authors": [
        {
            "name": "Jose H. MilÃ¡n",
            "email": "jhmilan@gmail.com",
            "homepage": "http://www.jmilan.net",
            "role": "CEO, Co-Founder"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": ">=5.4.0",
        "laravel/scout": "^3.0",
        "elasticsearch/elasticsearch": "^5.1"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "BittenByte\\ScoutElasticsearchEngine\\": "src"
        }
    },
    "autoload-dev": {
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "suggest": {
        "elasticsearch/elasticsearch": "Required to use the Elasticsearch engine (^5.1)."
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
